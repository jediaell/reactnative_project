{
  "name": "template-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npx jetify",
    "precommit": "yarn lint",
    "prepush": "lint-staged",
    "preversion": "yarn lint",
    "start": "npx solidarity && ENVFILE=.env.dev npx react-native start -- --reset-cache",
    "android": "npx solidarity && yarn clean:android && adb reverse tcp:9090 tcp:9090 && ENVFILE=.env.dev npx react-native run-android",
    "android:prd": "yarn clean:android && adb reverse tcp:9090 tcp:9090 && ENVFILE=.env.prd npx react-native run-android --variant=release",
    "android:hml": "yarn clean:android && adb reverse tcp:9090 tcp:9090 && ENVFILE=.env.hml npx react-native run-android --variant=releaseStaging",
    "ios": "npx solidarity && npx pod-install && ENVFILE=.env.dev npx react-native run-ios --scheme='DEV'",
    "ios:hml": "npx pod-install && ENVFILE=.env.hml npx react-native run-ios --scheme='HML'",
    "ios:prd": "npx pod-install && ENVFILE=.env.prd npx react-native run-ios --scheme='PRD'",
    "dist:ios:prd": "bundle exec fastlane ios prd",
    "dist:ios:hml": "bundle exec fastlane ios hml",
    "dist:android:prd": "bundle exec fastlane android prd",
    "dist:android:hml": "bundle exec fastlane android hml",
    "dist:codepush": "yarn codepush:ios && yarn codepush:android",
    "codepush:ios": "PACKAGE_VERSION=$(node -p -e \"require('./package.json').version\") && appcenter codepush release-react -a platformbuilders/Template-iOS -t $PACKAGE_VERSION",
    "codepush:android": "PACKAGE_VERSION=$(node -p -e \"require('./package.json').version\") && appcenter codepush release-react -a platformbuilders/Template-Android -t $PACKAGE_VERSION",
    "clean": "npx react-native clean-project-auto && yarn install && npx pod-install",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "resources": "yarn resources:icon && yarn resources:splash",
    "resources:icon": "react-native set-icon --platform ios --path icon.png",
    "resources:splash": "react-native set-splash --path splash.png --resize contain --background '#212121'",
    "assemble:android": "cd android && ENVFILE=.env.prd ./gradlew assembleRelease && mv app/build/outputs/apk/release/app-release.apk ../app-release.apk",
    "assemble:android:dev": "cd android && ENVFILE=.env.dev ./gradlew assembleReleaseStaging && mv app/build/outputs/apk/releaseStaging/app-releaseStaging.apk ../app-releaseDev.apk",
    "assemble:android:hml": "cd android && ENVFILE=.env.hml ./gradlew assembleReleaseStaging && mv app/build/outputs/apk/releaseStaging/app-releaseStaging.apk ../app-releaseStaging.apk",
    "bundle:android": "cd android && ENVFILE=.env.prd ./gradlew bundleRelease && mv app/build/outputs/bundle/release/app-release.aab ../app-release.aab",
    "install:android": "yarn bundle:android && adb install -r ./app-release.apk",
    "install:android:hml": "yarn bundle:android:hml && adb install -r ./app-releaseStaging.apk",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "tsc && eslint '*/**/*.{js,jsx,ts,tsx}' --fix",
    "version:build": "react-native-version -b",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "detox:ios": "yarn start & (detox build --configuration ios.sim.debug && detox test --configuration ios.sim.debug)",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "yarn pretty",
      "yarn lint",
      "yarn test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "dependencies": {
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@platformbuilders/helpers": "^0.3.2",
    "@platformbuilders/react-elements": "^0.0.23",
    "@platformbuilders/use-security": "^0.1.1",
    "@platformbuilders/validations": "^0.0.7",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "@react-native-firebase/analytics": "^8.0.1",
    "@react-native-firebase/app": "^9.0.0",
    "@react-native-firebase/crashlytics": "^8.5.2",
    "@react-native-firebase/iid": "^7.4.12",
    "@react-native-firebase/perf": "^7.4.12",
    "@react-native-firebase/remote-config": "^10.0.1",
    "@react-navigation/native": "^5.8.9",
    "@react-navigation/stack": "^5.12.6",
    "@types/react-native": "^0.63.34",
    "@types/styled-components": "5.1.4",
    "axios": "^0.21.0",
    "cpf_cnpj": "^0.2.0",
    "formik": "^2.2.5",
    "lodash": "^4.17.20",
    "lottie-ios": "3.1.9",
    "lottie-react-native": "3.5.0",
    "mobx": "5.15.7",
    "mobx-persist": "^0.4.1",
    "mobx-react": "6.3.1",
    "numeral": "^2.0.6",
    "polished": "^4.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-native": "0.63.3",
    "react-native-camera": "^3.40.0",
    "react-native-check-box": "^2.1.7",
    "react-native-check-version": "^1.0.8",
    "react-native-code-push": "^6.4.0",
    "react-native-config": "^1.4.1",
    "react-native-confirmation-code-input": "^1.0.4",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^7.0.2",
    "react-native-exit-app": "^1.1.0",
    "react-native-fast-image": "8.3.3",
    "react-native-flash-message": "^0.1.17",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-global-props": "^1.1.5",
    "react-native-haptic": "^1.0.1",
    "react-native-image-picker": "2.3.4",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-keychain": "^6.2.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-masked-text": "duspada/react-native-masked-text",
    "react-native-material-textfield": "^0.16.1",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.1",
    "react-native-responsive-screen": "^1.4.1",
    "react-native-rsa-native": "^2.0.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.14.0",
    "react-native-size-matters": "^0.3.1",
    "react-native-smooth-pincode-input": "^1.0.9",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-touch-id": "^4.4.1",
    "react-native-typography": "^1.4.1",
    "react-native-vector-icons": "^7.1.0",
    "react-native-version-number": "^0.3.6",
    "styled-components": "5.2.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/detox": "^16.4.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.56",
    "@types/react-native-check-box": "^2.1.1",
    "@types/react-native-datepicker": "^1.7.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/yup": "0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "babel-preset-react-native": "^4.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "detox": "^17.11.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.2",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "jetifier": "^1.6.6",
    "lint-staged": "^10.5.1",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^2.1.2",
    "prettierrc": "0.0.0-5",
    "react-dom": "^16.14.0",
    "react-native-clean-project": "^3.4.0",
    "react-test-renderer": "^16.14.0",
    "reactotron-react-native": "^5.0.0",
    "solidarity": "^3.0.0",
    "solidarity-react-native": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/template/Build/Products/Debug-iphonesimulator/template.app",
        "build": "xcodebuild -workspace ios/template.xcworkspace -configuration Debug -scheme template -sdk iphonesimulator -derivedDataPath build/template",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      }
    },
    "test-runner": "jest"
  },
  "jest": {
    "preset": "react-native",
    "automock": false,
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 35,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/assets/**",
      "!src/stores/**",
      "!src/config/**"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
